{
  "name": "@cosmoosjs/hono-openapi",
  "version": "1.0.1",
  "description": "Http server using hono zod for @cosmoosjs/core",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ae-creator/CosmosJS.git#main"
  },
  "bugs": {
    "url": "https://github.com/ae-creator/CosmosJS/issues"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --minify --target=bun && bun run build:declaration",
    "build:declaration": "bunx tsc --emitDeclarationOnly --outDir ./src",
    "dev": "bun run --hot src/index.ts"
  },
  "dependencies": {
    "@cosmoosjs/core": "^1.0.2",
    "@hono/swagger-ui": "^0.2.1",
    "@hono/zod-openapi": "^0.9.8",
    "hono": "^4.1.1",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "^1.0.8"
  }
}
