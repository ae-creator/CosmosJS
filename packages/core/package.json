{
  "name": "@cosmoosjs/core",
  "description": "create backend applications quickly and easily with bun.sh",
  "version": "1.0.2",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ae-creator/CosmosJS.git#main"
  },
  "bugs": {
    "url": "https://github.com/ae-creator/CosmosJS/issues"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --minify --target=bun && bun run build:declaration",
    "build:declaration": "bunx tsc --emitDeclarationOnly --outDir ./src",
    "test": "bun test",
    "lint": "bunx biome lint ./src"
  },
  "dependencies": {
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.1",
    "typescript": "^5.4.2",
    "zod": "^3.22.4",
    "pino": "^8.19.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.8",
    "pino-pretty": "^10.3.1"
  }
}
